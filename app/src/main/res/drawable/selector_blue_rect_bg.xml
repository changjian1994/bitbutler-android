<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_focused="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/position_sell_dialog_select_color" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/position_sell_dialog_select_color" />
        </shape>
    </item>

    <item android:state_selected="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/position_sell_dialog_select_color" />
        </shape>
    </item>

    <item>
        <shape>
            <stroke android:width="1dp" android:color="@color/position_sell_dialog_unselect_color" />
        </shape>
    </item>

</selector>